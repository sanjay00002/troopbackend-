{
    "name": "troopbackend",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "start:backend": "nodemon --exec babel-node ./backend/server.js",
        "start:websocket": "nodemon --exec babel-node ./websockets/server.js",
        "generate:token": "node ./websockets/stockSocket/generate_token.mjs",
        "migrate": "npx sequelize-cli db:migrate",
        "undo-migration": "npx sequelize-cli db:migrate:undo",
        "undo-all-migration": "npx sequelize-cli db:migrate:undo:all",
        "generate-migration": "npx sequelize-cli migration:generate --name"
    },
    "workspaces": [
        "backend/*",
        "database/*",
        "websockets/*"
    ],
    "devDependencies": {
        "@babel/cli": "^7.23.0",
        "@babel/core": "^7.23.2",
        "@babel/node": "^7.0.0",
        "@babel/preset-env": "^7.22.10",
        "@faker-js/faker": "^8.0.2",
        "@types/nanoid-dictionary": "^4.2.0",
        "nodemon": "^3.0.1"
    },
    "dependencies": {
        "axios": "^1.5.1",
        "binary-parser": "^2.2.1",
        "crypto": "^1.0.1",
        "dotenv": "^16.3.1",
        "multer": "^1.4.5-lts.1",
        "nanoid": "^3.3.6",
        "nanoid-dictionary": "^4.3.0",
        "node-fetch": "^2.7.0",
        "pg": "^8.11.2",
        "pg-hstore": "^2.3.4",
        "sequelize": "^6.32.1",
        "smartapi-javascript": "^1.0.16",
        "ws": "^8.14.2"
    }
}
